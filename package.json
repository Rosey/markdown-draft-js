{
  "name": "markdown-draft-js",
  "description": "Convert draftjs blocks to markdown using the marked library, and vice versa.",
  "author": "Rose Robertson",
  "version": "2.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rosey/markdown-draft-js.git"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "babel src --out-dir lib && NODE_ENV=esm babel src --out-dir esm",
    "prepublish": "npm run build",
    "test": "karma start --single-run",
    "lint": "eslint ./src",
    "lint:tests": "eslint ./test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run lint:tests"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.3.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.2.0",
    "karma-browserify": "^6.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^2.0.1",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "remarkable": "^2.0.1"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
